#http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = false

# Default format for javascript, python and json files
[*.{js,py,json}]
charset = utf-8
indent_style = tab
indent_size = 3

# Tab indentation (no size specified)
[Makefile]
indent_style = tab
